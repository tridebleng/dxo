[Unit]
Description=WebSocket By ePro
Documentation=https://tridebleng.com
After=syslog.target network-online.target

[Service]
User=root
NoNewPrivileges=true
ExecStart=/usr/local/tridebleng/ws-epro -f /etc/tridebleng/ws-epro.conf
Restart=on-failure
RestartPreventExitStatus=23
LimitNPROC=10000
LimitNOFILE=1000000

[Install]
WantedBy=multi-user.target