[Unit]
Description=OHP Mini ( OpenVPN )
Documentation=https://tridebleng.com
After=network.target nss-lookup.target

[Service]
User=root
NoNewPrivileges=true
ExecStart=/usr/local/tridebleng/ohp-mini -port 8080 -proxy 127.0.0.1:3128 -tunnel 127.0.0.1:1194
Restart=on-failure

[Install]
WantedBy=multi-user.target